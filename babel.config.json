{
  "presets": [
    ["@babel/preset-env",{ "targets": { "node": true }}],
    "@babel/react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "formatjs",
      {
        "idInterpolationPattern": "[sha512:contenthash:base64:6]",
        "ast": true
      }
    ],
    ["babel-plugin-styled-components"],
    ["babel-plugin-graphql-tag"]
  ]
}
